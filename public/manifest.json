{
  "manifest_version": 2,
  "name": "XMR.TO",
  "description": "Chrome extension",
  "version": "1.0.1",
  "content_scripts": [
    {
      "js": ["content.js" ],
      "css": ["content.css" ],
      "matches": [ "\u003Call_urls>" ],
      "all_frames": false
    }
  ],
  "permissions": [
    "activeTab",
    "https://xmr.to/",
    "notifications"
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "images/icon32.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "web_accessible_resources": [
    "*.gif",
    "*.png",
    "*.svg",
    "/static/css/main.css",
    "/static/js/1.chunk.js",
    "/static/js/main.js",
    "index.html"
  ]
}
